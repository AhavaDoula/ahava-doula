  <div class="col">
    <h1 class="title">Get in Touch</h1>
    <form action="mailto:jessie@ahavadoulaservices.com" method="post" enctype="text/plain">
      <span class="form-row">
        <label>Name</label>
        <input required type="text" name="name" class="input">
      </span>
      <span class="form-row">
        <label>Email</label>
        <input required type="text" name="email" class="input">
      </span>
      <span class="form-row">
        <label>Arrival Date</label>
        <input type="text" name="arrival-date" class="input">
      </span>
      <span class="form-row">
        <label>Message</label>
        <textarea class="input" name="message">
        </textarea>
      </span>
      <input type="submit" value="Send" class="submit-btn">
    </form>
  </div>
